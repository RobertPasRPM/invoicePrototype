@model InvoicePrototype.Models.FullAndPartialViewModel
@if(Model.InvoiceRows!=null){
foreach(var item in Model.InvoiceRows){
<tr>
    <td style="text-align:center;">@item.Index</td>
    <td style="text-align:center;">@item.Description</td>
    <td style="text-align:center;">@item.Quantity</td>
    <td style="text-align:center;">@item.UnitPrice</td>
    <td style="text-align:center;">@item.Tax</td>
    <td style="text-align:center;">@item.TotalTax </td>
    <td style="text-align:center;">@item.Total</td>
</tr>
}
}